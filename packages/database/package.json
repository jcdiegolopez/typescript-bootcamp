{
    "name": "@repo/db",
    "version": "0.0.1",
    "dependencies": {
        "@prisma/client": "5.22.0",
        "@repo/db": "file:"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/node": "^22.5.1",
